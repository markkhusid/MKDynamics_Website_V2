    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"ofKEp9sf2s"}],"key":"KJYZMykVjQ"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"GBaIsykPFy"}],"key":"QTY6xKzHKQ"}],"key":"xneeYD0hne"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"TWEc0dNiw9"},{"type":"outputs","id":"lI0f5TplaAZdkyew-5W2H","children":[],"key":"yg4zS0LRuV"}],"key":"abLPSe43eD"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"R6hxgsJGXk"},{"type":"outputs","id":"g4CmOY1yTABbE8QoW9o08","children":[],"key":"lUqskiS6ri"}],"key":"AEcbwPl4Ml"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"FfYWZMWuZa"},{"type":"outputs","id":"PWBwgHMPT7bq2CgWdBBty","children":[],"key":"PBPUMPc26F"}],"key":"trVkDn1ybA"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"x7kttSMbgu"},{"type":"outputs","id":"KZQhw-srad89o5axskVx8","childr