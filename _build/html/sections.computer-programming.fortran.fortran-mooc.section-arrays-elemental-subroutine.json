    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"OwBEKmjGfc"}],"key":"whWntILtzw"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"xx8nLcqpdM"}],"key":"uSvyuRFoSe"}],"key":"GVxRuRmQNG"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"qH0KhCvRpy"},{"type":"outputs","id":"dvI1ryYS-YLnAGrwYr-KV","children":[],"key":"mriuwBXbPt"}],"key":"w8mRAC3xMM"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"H1v5svft4c"},{"type":"outputs","id":"b3RqHIC0H9NXivIzZWADm","children":[],"key":"MmVVd3WH10"}],"key":"qMUiLzSxbd"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"eQtalCetvQ"},{"type":"outputs","id":"pD-Mt1BQFI8roO58isozO","children":[],"key":"ZcEG84ANv0"}],"key":"IQQG9PWuCS"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"Kv2cbLLH8r"},{"type":"outputs","id":"Cp_3EMeLmegKre1Z7x4II","childr