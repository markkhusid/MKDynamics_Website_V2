    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"VwcA5ab8hM"}],"key":"pJyhdFH0Kr"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"YCp6gh1lJP"}],"key":"uQmhNqSYxl"}],"key":"inkApmHzNF"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"C5ozZJKzOO"},{"type":"outputs","id":"b6D1YRd7_UAQjILkv5kBp","children":[],"key":"A1C6MzAUrm"}],"key":"JXxQV6uHur"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"ggWP5pqLRH"},{"type":"outputs","id":"NYgtBllAt6DLkqnBXM0ij","children":[],"key":"k2mhOpW5V6"}],"key":"MTSRohPNH9"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"waGDsXqToo"},{"type":"outputs","id":"-jPPW9f0CGR4jCZemwGfB","children":[],"key":"gTbgkY6hA7"}],"key":"qSHHlchHNA"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"Zl9sFfxKT0"},{"type":"outputs","id":"lBM7AYtnFOL3RAMbsZ6IO","childr