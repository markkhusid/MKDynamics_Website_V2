    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"BNp1T6eG4g"}],"key":"g24oOanyTd"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"BvmMB5UdCH"}],"key":"ILMyPLvnD6"}],"key":"W1XQefOuvt"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"XJ3joYwExW"},{"type":"outputs","id":"FzANsaowwxBFEY8XtA2_v","children":[],"key":"odNExzp8tl"}],"key":"dKVpcK7Ply"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"IsiqFQ1IwU"},{"type":"outputs","id":"Izr_wn3KatAysq31KB_ZL","children":[],"key":"ahrCm2Psvt"}],"key":"Pp6mfTaTt3"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"CgKwMlTqY1"},{"type":"outputs","id":"inlXqkXEorUB9A5grjQ6t","children":[],"key":"m1ocbYIHEt"}],"key":"uIgWD1YDMu"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"kKLbzlD5s5"},{"type":"outputs","id":"yMDxkX37TJzD33mo-a_0T","childr