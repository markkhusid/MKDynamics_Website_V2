    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"ZiwQevWDrQ"}],"key":"kIww9W0NMF"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"PchSvDeMeL"}],"key":"JkjrMrCOyS"}],"key":"raehIgVSSW"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"PXYm4HsR9F"},{"type":"outputs","id":"3UccJ0fVIXDNX5Kd0CDEj","children":[],"key":"Q4GWrbHhWZ"}],"key":"IWhXgBolBK"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"vQpBhyBbf9"},{"type":"outputs","id":"y2Ccx_L3wDMusScV63_BA","children":[],"key":"HD1lIAl3vR"}],"key":"qTZwm7OeGK"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"voYQUov3dY"},{"type":"outputs","id":"DilJ7ghVPcl5b6aZeMVjU","children":[],"key":"rnmqX2vogs"}],"key":"ZofE5WLoyo"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"syhKiGrl1p"},{"type":"outputs","id":"OARM5ojMQ7c_eXxk6ULYQ","childr