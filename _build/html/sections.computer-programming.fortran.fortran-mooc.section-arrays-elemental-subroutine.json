    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"XXYNjYUrwS"}],"key":"u7y4aA4XxR"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"v2fLCF471n"}],"key":"GXj33EBRfQ"}],"key":"cOdRM5FL3X"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"lGaG4za8st"},{"type":"outputs","id":"w3FvUhyEKR1-tFfPV7pJt","children":[],"key":"cSgcytV97l"}],"key":"N5zfo8y7fl"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"RKgafDwKEQ"},{"type":"outputs","id":"ieQGhT4pBfUbSnDj8jHoa","children":[],"key":"WFW08lEOpr"}],"key":"PaMOJxb6ii"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"EmVudYPJya"},{"type":"outputs","id":"9d9wkrT2n9PTYb33Ksq4Q","children":[],"key":"qPmgXuCNQJ"}],"key":"U6mmeQ0kRV"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"bHDgqV7l3D"},{"type":"outputs","id":"4B_VZ9mr8hEy-DPie1TL8","childr