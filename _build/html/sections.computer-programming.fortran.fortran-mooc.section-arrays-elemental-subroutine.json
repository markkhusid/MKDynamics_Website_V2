    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"fSAZjJo4yD"}],"key":"ZwaUPrVV8G"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"uFY7QArCDH"}],"key":"f1HeILKL7O"}],"key":"v11PIybolW"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"RqzDrmzzsJ"},{"type":"outputs","id":"xNSTqv-ubugvtnMeKh-FU","children":[],"key":"z8BZaqvqNb"}],"key":"bokPwetCbO"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"Te742qf2Uk"},{"type":"outputs","id":"wo27nrpJ7Xc43s3GaS0wP","children":[],"key":"UwPCcQ1XdP"}],"key":"UmtaEb8QgO"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"Kh3oD5rgGr"},{"type":"outputs","id":"HlRAWvfxQ9azgYakdb73-","children":[],"key":"GITEe8UsB4"}],"key":"jv5DjEOIAg"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"wNQflvbh6S"},{"type":"outputs","id":"UIG8S1ca4p6gH3yCKepIH","childr