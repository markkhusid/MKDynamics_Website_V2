    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"moReW1GXor"}],"key":"CGX3RgvThP"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"pGPMvlZ6yx"}],"key":"Ijtlhf1ROJ"}],"key":"qEsh6oodeI"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"C9yUoGpz0B"},{"type":"outputs","id":"08wcRd5RP656L_lh4ITXn","children":[],"key":"JrMo1VxIoG"}],"key":"lJv4XDR8Np"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"D9MAyYSwQL"},{"type":"outputs","id":"a05bUP-Sx7khRXSyaQHaK","children":[],"key":"C2nHWgYMpT"}],"key":"YAuiIkS4at"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"MBH86HREkF"},{"type":"outputs","id":"pC3WipSIcYUxenwIEKoat","children":[],"key":"iyOiiYT3iN"}],"key":"s22Ky6suKj"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"xpp7Jd9QxL"},{"type":"outputs","id":"JNBbkTMwTTfmJjAzQFKe7","childr