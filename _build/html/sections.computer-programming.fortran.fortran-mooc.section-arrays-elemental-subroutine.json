    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"UMWfZyVQZW"}],"key":"d0U5vg3zz7"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"STK9xOX3i8"}],"key":"ooKpmgP0Zl"}],"key":"TMlHv8FMwc"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"oHFrKcDn0E"},{"type":"outputs","id":"7R8IJOQUCYtwLwOZ2SckY","children":[],"key":"R2sEPczm53"}],"key":"i93gwx0eRX"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"zdz2VhzoVz"},{"type":"outputs","id":"0ZxwSPfB1NydyvPCkgFba","children":[],"key":"SJe6LDaCJt"}],"key":"zCcioQDLgm"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"YNIeEiGbzn"},{"type":"outputs","id":"rC1FH2kcIW4gXWH3mthbt","children":[],"key":"DiZle28xnd"}],"key":"M1wvRdSA9g"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"lUAPf9qRjb"},{"type":"outputs","id":"bZ6qARZNLs0WFbNq4AYFa","childr