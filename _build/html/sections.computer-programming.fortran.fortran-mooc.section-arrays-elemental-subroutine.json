    do i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"MSR6cUJf6y"}],"key":"Rw1yrQFSp2"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"NkAVWGsRsW"}],"key":"omvohAr6Vq"}],"key":"tx9a2CXnVN"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"wreVCxwCZB"},{"type":"outputs","id":"2VgyFPTUWoayOgWgz7iEd","children":[],"key":"RCkg0J3pGT"}],"key":"To47uClmcD"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"oO3qycT0ef"},{"type":"outputs","id":"g5EKKM3Rg3JDkZAgjCG2d","children":[],"key":"HlcUddS1ON"}],"key":"DmzEde6TX0"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"emLVEWAka2"},{"type":"outputs","id":"v1fHe5A1CoB7tFiho2PuW","children":[],"key":"ClBNWTVOZN"}],"key":"GBKhmZaWRa"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"WlOertiJ07"},{"type":"outputs","id":"Mms5H739vlLnpQ2vhqHl2","childr