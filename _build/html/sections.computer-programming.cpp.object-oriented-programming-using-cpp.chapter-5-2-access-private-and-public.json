l\n\n    while (str[i] && !s.full())\n    {\n        s.push(str[i++]);\n    }\n\n    while (!s.empty())      // print the reverse\n    {\n        cout << s.pop();\n    }\n\n    cout << endl;\n}","position":{"start":{"line":1,"column":1},"end":{"line":29,"column":1}},"key":"lTC2kULWHV"}],"key":"bm9vLQRC22"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"ch_stac3.h","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"yIRQG8VB6E"}],"key":"ZkWnDQUVFH"}],"key":"cYFOaH8JfL"},{"type":"block","kind":"notebook-content","children":[{"type":"code","lang":"c++","value":"struct ch_stack {\n    public:\n        void reset ()       { top = EMPTY; }\n        void push (char c)  { top++; s[top] = c; }\n        char pop ()         { return s[top--]; }\n        char top_of ()      { return s[top]; }\n        bool empty ()       { return ( top == EMPTY ); }\n        bool full ()        { return ( top == FULL  ); }\n\n    private:\n        enum \n        {  \n            max_len = 100,\n            EMPTY = -1,\n            FULL = max_len-1\n                \n        };\n\n        char s[max_len];\n        int top;\n};","position":{"start":{"line":1,"column":1},"end":{"line":23,"column":1}},"key":"DauZ4Uc0yO"}],"key":"fxvVdS4FKp"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Gnu Compiler Collection (g++):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"yaavcIMjxu"}],"key":"lAXSnwpFAB"}],"key":"VwcJeu3Brr"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"OHw5U1TdKk"},{"type":"outputs","id":"zvvBbDnv8S-RfYwIwHS5_","children"