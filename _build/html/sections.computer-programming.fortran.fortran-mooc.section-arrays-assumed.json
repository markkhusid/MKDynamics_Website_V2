, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"DS15cbmq5l"}],"key":"XdkimQavwC"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"ZS27v2kcLJ"}],"key":"VtuVKQ8I1i"}],"key":"OIQ3Kdrps2"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"mmMTIAxz0Z"},{"type":"outputs","id":"jaNCxYUSWBUpMN2xOEg4z","children":[],"key":"O7qKfM2SwB"}],"key":"L1jZoFz7TL"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"BhLsSG5vC8"},{"type":"outputs","id":"2ZFG1Gl_qmpoXmRr95Vez","children":[],"key":"ey9YrhT257"}],"key":"nZWGbtEtSm"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"uACx8qDPt3"},{"type":"outputs","id":"XTHjxjyLKx8ojxIVJV-uZ","children":[],"key":"osnEPufXF9"}],"key":"puQCNRY4ev"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"gYKf1Sut0l"},{"type":"outputs","id":"7bfc3B9X_v5rSzPnFHtyh","children":[],"key":"hvPjzb5kJY"}],"key":"aBMRdVHZJb"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"C1DEN1VM4L"},{"type":"outputs","id":"d3cgg