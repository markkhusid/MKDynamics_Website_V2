, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"a3QQzRC7Y5"}],"key":"Qp3rfIWmOC"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"fCOkZ5Cfex"}],"key":"TMO1CouG0X"}],"key":"xtCNXGs0jC"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"UCL1JSQyoI"},{"type":"outputs","id":"cKiB-uQyqzacyzYAQnANW","children":[],"key":"c4SWeJwJWe"}],"key":"kpatva43k5"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"mH6YaCkevN"},{"type":"outputs","id":"lXGluvEkD0rBNZNJ-j2H2","children":[],"key":"H8gPzgLLHK"}],"key":"QfK35lQlY3"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"YODus84rlW"},{"type":"outputs","id":"Qi4sttiWGtFXiO7x1BOi5","children":[],"key":"ZceXdrKAfa"}],"key":"Ez1HcA9Ka3"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"je2HsiTL7U"},{"type":"outputs","id":"x4SO3BfS7NmIXUCKs9VmA","children":[],"key":"Sh7YAFgQOT"}],"key":"vTJ6Kegv5D"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"lJ0fQCx3C2"},{"type":"outputs","id":"r7nqS