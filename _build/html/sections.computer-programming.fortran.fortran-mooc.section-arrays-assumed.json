, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"f0osHWoxKG"}],"key":"uvfDeEzpfz"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"kCSp2sED0A"}],"key":"NIo3iMHyi0"}],"key":"NJVRLBspBG"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"FQzympzWv6"},{"type":"outputs","id":"Pift2NlzFxnn2JvUpAh2X","children":[],"key":"S8tR3tqYwz"}],"key":"Ym1yf8Yo7V"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"LHrDauY8XN"},{"type":"outputs","id":"lHrM_ELWGyPCxwPhtFlwM","children":[],"key":"pgnenq9PYv"}],"key":"lU3XwNJrKI"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"svzGNiXR1N"},{"type":"outputs","id":"fd-s6AN8aR0t7WempG7jB","children":[],"key":"hpachqWyfc"}],"key":"Is0rzdqURT"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"zEDNOgaQAd"},{"type":"outputs","id":"pwX-58BOBQ07hCNyU3vPp","children":[],"key":"QMl2ObjE4J"}],"key":"macmF1Cna4"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"a44iUtyZ1e"},{"type":"outputs","id":"ww38h