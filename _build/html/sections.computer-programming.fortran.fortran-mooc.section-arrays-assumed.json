, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"DsAHVYsz2v"}],"key":"LO0ajeXi4z"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"CMb6sPZGE1"}],"key":"wpCBdMffxy"}],"key":"p9KmXtpywS"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"arTp58RhIq"},{"type":"outputs","id":"Elyd7rSeG9_Hg6qYVNbzT","children":[],"key":"iopyLYtB4V"}],"key":"IYrxew5i1W"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"t9w1qFSktA"},{"type":"outputs","id":"KtkoTHSsNyBEMjt4yHWV3","children":[],"key":"mgYxO9J8kT"}],"key":"Xyc6XLgMbW"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"x6qAuK2vbQ"},{"type":"outputs","id":"kQNg5EUfK_pZpSNFv-IsH","children":[],"key":"rlXM69wCvE"}],"key":"kFJnh2Nika"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"eEHgPfK7RG"},{"type":"outputs","id":"vwCcrb4vu8JrbVRENXuCQ","children":[],"key":"yxaPeGaeTg"}],"key":"jnkMZasJyb"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"iy8tv13uuF"},{"type":"outputs","id":"G0g6w