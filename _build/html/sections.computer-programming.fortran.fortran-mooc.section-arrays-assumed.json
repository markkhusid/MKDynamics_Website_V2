, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"QEQjbCXG0Y"}],"key":"CNeBnH1SWz"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"tLwxaDsdBT"}],"key":"Pne08AYn8N"}],"key":"bZSwIiBCnc"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"I32lVGgKG3"},{"type":"outputs","id":"Jk-K4mUz6XeT0wCDHnnwy","children":[],"key":"bGYkOjREg1"}],"key":"EtyrjIyQ19"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"CEgEOwkbrt"},{"type":"outputs","id":"5hB0z-sPxIQkY9uSeoU7X","children":[],"key":"XVtE2YdPTJ"}],"key":"IVn60GFK8E"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"DVbIBQUK0e"},{"type":"outputs","id":"HA_rpEXd1vmh_VTMQlgYT","children":[],"key":"U4o1TbbhoD"}],"key":"evkyvAyu5t"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"uu7jIrGxYR"},{"type":"outputs","id":"GQOlaq4FAxVbTzp8dDc7R","children":[],"key":"G84hqJM0in"}],"key":"dtkIp8FpRy"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"Y11L1zeGtR"},{"type":"outputs","id":"YMTng