, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"e4dlQeilXx"}],"key":"NsilDqtqQb"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"t7faQ3duCO"}],"key":"Z6G4gHNuwO"}],"key":"HiZv5IFeoJ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"kLMjzXSjUV"},{"type":"outputs","id":"5Yb7Gf3R1kKMrCvuYZr0t","children":[],"key":"R00cpZeLrL"}],"key":"MPlCKDUErU"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"OcTLOYXG96"},{"type":"outputs","id":"uQnNlkW_aEa_GNDV942Ky","children":[],"key":"APcBcCdbWc"}],"key":"c6YnDvHgqc"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"Ij9MtGEU71"},{"type":"outputs","id":"h2dVDJNVdgVy8KhrUNZ6g","children":[],"key":"VzEyFNJ1sG"}],"key":"xcrzDUWgLS"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"zLpNAD0lu5"},{"type":"outputs","id":"k0Q7uGP5dm-6Vw-eYMWVr","children":[],"key":"xkw1N7wRB8"}],"key":"tLDGy47kkI"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"gaReGAW84j"},{"type":"outputs","id":"jjxPA