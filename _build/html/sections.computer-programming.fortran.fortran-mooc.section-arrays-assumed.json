, dimension(:), intent(inout) :: A\n        integer :: i\n\n        do i = 1, size(A)\n            A(i) = fac(A(i))\n        end do\n    end subroutine factorial\n\nend program assumed","position":{"start":{"line":1,"column":1},"end":{"line":34,"column":1}},"key":"QB5qzTR43u"}],"key":"vURr4wCFFN"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"JdfBpr3W6e"}],"key":"GqUYNEV1YD"}],"key":"wj0iedow4u"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"XpKCiU0VHu"},{"type":"outputs","id":"-WshpaSv86KDtU8abaVbo","children":[],"key":"OrMJDs0uNO"}],"key":"tBHWlCyetU"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Assumed\"","key":"bF1Gpgyk9t"},{"type":"outputs","id":"C42SHKRPdDCYmuLbxhTPe","children":[],"key":"ebxjIObEEy"}],"key":"AhzJIhp72t"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"yaNEYptSDw"},{"type":"outputs","id":"VypRXN5sS4Oohc7TyEhaj","children":[],"key":"ZsAgap4Aiv"}],"key":"VpOzqHnKOj"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"sQBl6Aaoy8"},{"type":"outputs","id":"2qBCyh4vyHhTD-PoA0UFL","children":[],"key":"VNiIss6uG0"}],"key":"difizz9Vvy"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"exec_status = os.system(\"fpm run 2>/dev/null\")","key":"wejgrRzdmB"},{"type":"outputs","id":"cD91c