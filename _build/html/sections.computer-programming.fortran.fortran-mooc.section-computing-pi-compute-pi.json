ers\n    x = 0.0_DP\n    do i = 1, nr_iters\n        pi_val = pi_val + sqrt(1.0_DP - x**2)\n        x = x + delta\n    end do\n    pi_val = 4.0_DP*pi_val/nr_iters\n    print '(A, I10, A, F25.15)', \"After \", nr_iters, \" loops, Pi = \", pi_val\n    print '(A, F25.15)', \"Actual value of Pi = \", pi_val_actual\n    print '(A, F25.15)', \"Absolute difference = \", abs(pi_val - pi_val_actual)\n\ncontains\n     \n    function get_nr_iters() result(nr_iters)\n        use, intrinsic :: iso_fortran_env, only : error_unit\n        implicit none\n        integer(kind=I8) :: nr_iters\n        integer(kind=I8), parameter :: default_nr_iters = 1000_I8\n        character(len=1024) :: buffer, msg\n        integer :: istat\n\n        if (command_argument_count() >= 1) then\n            call get_command_argument(1, buffer)\n            read (buffer, fmt=*, iostat=istat, iomsg=msg) nr_iters\n            if (istat /= 0) then\n                write (unit=error_unit, fmt='(2A)') &\n                    'error: ', msg\n                stop 1\n            end if\n        else\n            nr_iters = default_nr_iters\n        end if\n    end function get_nr_iters\n\nend program compute_pi\n","lang":"fortran","filename":"section_computing_pi_compute_pi.f90","key":"SF6FNS7V04"}],"key":"ucaSb2sqJW"}],"key":"BnJ6WPbIDt"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"rv99FCHzmz"}],"key":"DbwEMeV6Ov"}],"key":"cCKJdjF2lZ"},{"type":"block","kind":"notebook-content","data":{"tags":[]},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Build the Program using FPM (Fortran Package Manager)","position":{"start":{"line":1,"