)\n\ncontains\n\n    function init_lookup_table() result(lookup_table)\n        implicit none\n        integer, dimension(0:255) :: lookup_table\n        integer :: i\n\n        lookup_table(0) = 0\n        do i = 1, 255\n            lookup_table(i) = and(i, 1) + lookup_table(i/2)\n        end do\n    end function init_lookup_table\n\nend program create_lookup_table\n","lang":"fortran","filename":"section_bit_manipulations_create_lookup_table.f90","key":"bjf5RY2TDn"}],"key":"eFBnXyac0G"}],"key":"Z19nggJ2wQ"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm).  The following FPM configuration file (fpm.toml) was used:","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Ph7A2kkUI3"}],"key":"Y0KsDdtDfq"}],"key":"LTYE0KhDtZ"},{"type":"block","kind":"notebook-content","children":[{"type":"include","file":"Fortran_Code/Section_Bit_Manipulations_Create_Lookup_Table/fpm.toml","literal":true,"children":[{"type":"code","value":"name = \"Section_Bit_Manipulations_Create_Lookup_Table\"\n\n[build]\nauto-executables = true\nauto-tests = true\nauto-examples = true\n\n[install]\nlibrary = false\n\n[[executable]]\nname=\"Section_Bit_Manipulations_Create_Lookup_Table\"\nsource-dir=\"app\"\nmain=\"section_bit_manipulations_create_lookup_table.f90\"\n","lang":"toml","filename":"fpm.toml","key":"x67fUKyDDi"}],"key":"jdyVC6NH2S"}],"key":"BstYW0SoGe"},{"type":"block","kind":"notebook-content","data":{"tags":[]},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Build the Program using FPM (Fortran Package Manager)","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"DZCM73Yp2l"}],"identifier":"build-the-program-using-fpm-fortran-package-manag