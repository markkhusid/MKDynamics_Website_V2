d interface\n\n  print '(A20, *(F5.1))', 'vector 1: ', vector1\n  print '(A20, *(F5.1))', 'vector 2: ', vector2\n  print '(A20, F6.2)', 'vector1 . vector2 = ', &\n      sdot(size(vector1), vector1, 1, vector2, 1)\n\n  print '(A)', 'matrix:'\n  do i = 1, size(matrix, 1)\n      print '(*(F5.1))', matrix(i, :)\n  end do\n  print '(A, F10.1)', 'col 2 . col 4:', &\n      sdot(size(matrix, 2), matrix(:, 2), 1, matrix(:, 4), 1)\n  print '(A, F10.1)', 'row 2 . row 4:', &\n      sdot(size(matrix, 1), matrix(2, :), 1, matrix(4, :), 1)\nend program dot_test\n","lang":"fortran","filename":"section_blas_lapack_dot.f90","key":"nk6s8QQxWK"}],"key":"jL0jDQoaTa"}],"key":"IeYg32U6EC"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"K1hTi4LbqZ"}],"key":"ALcAWQog1g"}],"key":"FnDhqR4C9i"},{"type":"block","kind":"notebook-content","data":{"tags":[]},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Build the Program using FPM (Fortran Package Manager)","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"sASvlaMzGl"}],"identifier":"build-the-program-using-fpm-fortran-package-manager","label":"Build the Program using FPM (Fortran Package Manager)","html_id":"build-the-program-using-fpm-fortran-package-manager","implicit":true,"key":"sFLpQRLaSq"}],"visibility":"show","key":"dLF4HHXAzM"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = \"\"\nroot_dir = os.getcwd()","key":"w9QNTXYUxj"},{"type":"outputs","id":"UIBrcxTvokTPrWUVWdBcD","children":[],"key":"oDKjICT4Mk"}],"key":"aMDSr5vXxu"},{"type":"block","kind":"