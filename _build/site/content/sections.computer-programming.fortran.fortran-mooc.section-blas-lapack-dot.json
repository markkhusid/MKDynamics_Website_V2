d interface\n\n  print '(A20, *(F5.1))', 'vector 1: ', vector1\n  print '(A20, *(F5.1))', 'vector 2: ', vector2\n  print '(A20, F6.2)', 'vector1 . vector2 = ', &\n      sdot(size(vector1), vector1, 1, vector2, 1)\n\n  print '(A)', 'matrix:'\n  do i = 1, size(matrix, 1)\n      print '(*(F5.1))', matrix(i, :)\n  end do\n  print '(A, F10.1)', 'col 2 . col 4:', &\n      sdot(size(matrix, 2), matrix(:, 2), 1, matrix(:, 4), 1)\n  print '(A, F10.1)', 'row 2 . row 4:', &\n      sdot(size(matrix, 1), matrix(2, :), 1, matrix(4, :), 1)\nend program dot_test\n","lang":"fortran","filename":"section_blas_lapack_dot.f90","key":"UCS8sK9u6b"}],"key":"LC7lUnre9c"}],"key":"QGE7vgRKo1"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"vO9KdhUemp"}],"key":"Jke1RWR0m7"}],"key":"r8MvqmU2yr"},{"type":"block","kind":"notebook-content","data":{"tags":[]},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Build the Program using FPM (Fortran Package Manager)","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"PPQxnghGye"}],"identifier":"build-the-program-using-fpm-fortran-package-manager","label":"Build the Program using FPM (Fortran Package Manager)","html_id":"build-the-program-using-fpm-fortran-package-manager","implicit":true,"key":"QuXooEzDdk"}],"visibility":"show","key":"NciBAD82Uj"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = \"\"\nroot_dir = os.getcwd()","key":"gHgoKEVXY2"},{"type":"outputs","id":"u-Z3Dp4uywyjtrVMLP_0J","children":[],"key":"RXTHB9dbgd"}],"key":"mUAuGgrlA0"},{"type":"block","kind":"