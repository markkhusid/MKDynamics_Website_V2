d interface\n\n  print '(A20, *(F5.1))', 'vector 1: ', vector1\n  print '(A20, *(F5.1))', 'vector 2: ', vector2\n  print '(A20, F6.2)', 'vector1 . vector2 = ', &\n      sdot(size(vector1), vector1, 1, vector2, 1)\n\n  print '(A)', 'matrix:'\n  do i = 1, size(matrix, 1)\n      print '(*(F5.1))', matrix(i, :)\n  end do\n  print '(A, F10.1)', 'col 2 . col 4:', &\n      sdot(size(matrix, 2), matrix(:, 2), 1, matrix(:, 4), 1)\n  print '(A, F10.1)', 'row 2 . row 4:', &\n      sdot(size(matrix, 1), matrix(2, :), 1, matrix(4, :), 1)\nend program dot_test\n","lang":"fortran","filename":"section_blas_lapack_dot.f90","key":"BPohpH90eQ"}],"key":"NrkVK63UVM"}],"key":"gx6HGIVrPa"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"X35IUAkWeK"}],"key":"l6PSPHuvry"}],"key":"x9EsCvzLqB"},{"type":"block","kind":"notebook-content","data":{"tags":[]},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Build the Program using FPM (Fortran Package Manager)","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"z2WCw4PR9Q"}],"identifier":"build-the-program-using-fpm-fortran-package-manager","label":"Build the Program using FPM (Fortran Package Manager)","html_id":"build-the-program-using-fpm-fortran-package-manager","implicit":true,"key":"Y9l5nw5o0h"}],"visibility":"show","key":"jflguD4R4I"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = \"\"\nroot_dir = os.getcwd()","key":"nmg1ycSkZu"},{"type":"outputs","id":"HUiHYgkZviYrp23Z_fA7c","children":[],"key":"qcNXcve85g"}],"key":"L6gPCLh6NC"},{"type":"block","kind":"