Sums = SqrOfSums ** 2\n\n    ! compute the difference between sum of squares and square of sums  \n    difference = SqrOfSums - SumOfSqrs\n\n    ! display results\n    print *\n    write (*,'(a)') \"Results...\"\n    print *\n    write (*, '(a, i5, a, i10)') \"The square of the sums from 1 to \", n, \" is -> \", SqrOfSums\n    print *\n    write (*, '(a, i5, a, i10)') \"The sum of the squares from 1 to \", n, \" is -> \", SumOfSqrs\n    print *\n    write (*,'(a, i10)') \"The difference between the square of the sums and sum of the squares is -> \", difference\n    print *\nend program SOSdifference\n","lang":"fortran","filename":"section_9_3_sos_difference.f90","key":"iFvBX6OhzR"}],"key":"hyUSSP0Xn6"}],"key":"eZLHO7lcTX"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The following explanation is given by “Code Copilot” within ChatGPT 4.","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Ifjyq9uAry"}],"key":"qlwtm8At4j"}],"key":"b675XaBPcp"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The Fortran program ","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"zOGp28Q3v5"},{"type":"inlineCode","value":"SOSdifference","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"EQc0EtjMqn"},{"type":"text","value":" is designed to calculate and display the difference between two mathematical quantities: the square of the sum of the first ","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"WIgPKEv4wV"},{"type":"inlineCode","value":"n","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Eit9DVo0sQ"},{"type":"text","value":" natural numbers and the sum of the squares of the first ","p