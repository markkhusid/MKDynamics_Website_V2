 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"jWYQjVFpaA"}],"key":"KTW7GHIuoJ"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"NysWhVilMQ"}],"key":"JrMMTyzmAh"}],"key":"kGcg6u0WVr"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"iZVAgOr8mm"},{"type":"outputs","id":"QTqy4y_P1NaCfxcFZqJee","children":[],"key":"NtlXFhvm1h"}],"key":"sfJeMBOOVS"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"cfOOjSydOO"},{"type":"outputs","id":"IDFA9hFFFux8dMWt2Ruqz","children":[],"key":"fR36o1sR2o"}],"key":"ENEQgRQuvD"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"Q7KOVE6Lrs"},{"type":"outputs","id":"GO1YWJ1r2_CE5XMbuL3Ij","children":[],"key":"WtuvnP1cVa"}],"key":"ZYFUx1G3lY"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"kklY0cYQrg"},{"type":"outputs","id":"FXjAquPk8W-D9LdnVaELm","children":[]