 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"RyLpfsQBRN"}],"key":"jBGyyKDxyR"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"mK33xJXQn3"}],"key":"j34Jt66Rof"}],"key":"sEidyHOYfq"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"cZ6sxod0CX"},{"type":"outputs","id":"uABTJGqXHrKPANpMyiPf6","children":[],"key":"KBmXeTjxKZ"}],"key":"zNb8jtHdvP"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"RPG1xis0nc"},{"type":"outputs","id":"F-J2FYgfslf699A1v3qXp","children":[],"key":"eIvrfiOyws"}],"key":"MSu04wRjU0"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"uCYhqqWaxf"},{"type":"outputs","id":"xTSeXIrS5SnubbNYqhT2k","children":[],"key":"IbFmmTaQbT"}],"key":"nNJhkSmBhn"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"s2dhvAoq9T"},{"type":"outputs","id":"gpJZgY6PZ_zAcjIIQ0o0G","children":[]