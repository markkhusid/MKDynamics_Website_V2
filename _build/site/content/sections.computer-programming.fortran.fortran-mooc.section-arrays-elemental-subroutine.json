 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"WvASmRmMcA"}],"key":"uCa6OViPQs"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"l1WQVkXNJu"}],"key":"gjdOGqp9X0"}],"key":"S8IcA8Biel"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"UVOM2ph1Xe"},{"type":"outputs","id":"LXVreI6Qfv56DDia1vpot","children":[],"key":"vj0QBQ5v2D"}],"key":"H03nivMWRl"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"YEfpvhNysY"},{"type":"outputs","id":"7Dahd1Y4Ie16b9oeCzd8P","children":[],"key":"bL5Bm2OP8k"}],"key":"FNCmu1BPSr"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"wKvIwaEjIV"},{"type":"outputs","id":"XDNhwjyeNQPHpbEzLYinK","children":[],"key":"rmRrHfRvQs"}],"key":"MdV4uGApiS"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"xDhWKLm1h0"},{"type":"outputs","id":"LvTX-WTIqPpUmSIVZf1_P","children":[]