 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"taVg6KMaC1"}],"key":"tYcpLzryHu"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"a5PPWjqL9V"}],"key":"lxK3EoQa6P"}],"key":"R2w99ERzbf"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"rctLZH4IhM"},{"type":"outputs","id":"uy6iL_z5Cp1oymrF-49LE","children":[],"key":"g2RadGAKgQ"}],"key":"MrCBIJSv3O"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"rUqyhZ4Dw0"},{"type":"outputs","id":"OZgn5n7VBScGIPOvP9d9p","children":[],"key":"IgFnL5RL5n"}],"key":"j2YlZcpGlg"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"OIXE8c05of"},{"type":"outputs","id":"ZoaUM14id3vdFUWRKxuUJ","children":[],"key":"AfUs4eLOWa"}],"key":"XcBNwkv6kM"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"IKU0gr6p4C"},{"type":"outputs","id":"6GQVMgjasWzugCuYVQKQA","children":[]