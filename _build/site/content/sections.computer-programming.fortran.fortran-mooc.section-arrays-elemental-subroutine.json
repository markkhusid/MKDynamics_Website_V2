 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"xOleHliJXA"}],"key":"SJhTktLhPa"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"dVD6745y9q"}],"key":"IZTWLskkhx"}],"key":"enEf8XmRag"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"nzTVzLrVh2"},{"type":"outputs","id":"D_VQDXd4ulDZhR-FO7ddz","children":[],"key":"XU3b55IScF"}],"key":"hBLvFxLElJ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"tWvZiOMybB"},{"type":"outputs","id":"NAZqkrBsIVQmjAAmziPx9","children":[],"key":"hoRBIfekOH"}],"key":"jZR9siemHV"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"uEKj09mZgw"},{"type":"outputs","id":"TeI4LJsctmhLZOARM5ni9","children":[],"key":"oddYqXCwYg"}],"key":"zAeOCpCpyr"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"WfMQDGnsWj"},{"type":"outputs","id":"wqzxLr8CyIf5-PYPSKduQ","children":[]