 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"VIz9saovxY"}],"key":"UJvvC82gtl"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"vp2eT3RpCZ"}],"key":"qqtYxYsNNu"}],"key":"ixyuwopjIi"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"phthAFo971"},{"type":"outputs","id":"BG9qKJbrL5-rsXeQRjkmt","children":[],"key":"wvNnzDSuwX"}],"key":"uvCLWrfVTZ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"yMpbU7afl6"},{"type":"outputs","id":"NPLU9xETVyZZf0BBusu7X","children":[],"key":"lEdgZDoN9X"}],"key":"sjLs5tANe4"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"GzmmeswN5y"},{"type":"outputs","id":"GP5j1WHj-9gbO5MKrBBwD","children":[],"key":"yHkuz7RQo6"}],"key":"ZwXQeY1zm0"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"ijttl2L0TM"},{"type":"outputs","id":"isJptDtwuoLBYd--1SRnH","children":[]