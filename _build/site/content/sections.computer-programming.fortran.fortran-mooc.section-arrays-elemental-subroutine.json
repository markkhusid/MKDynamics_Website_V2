 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"CdW7mFdIpu"}],"key":"J3ShXgoWEw"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"wNXFuooQs4"}],"key":"sCbAiyidgE"}],"key":"guF83x5SbL"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"urU0QRnxv2"},{"type":"outputs","id":"QsCXUJ1yontDajyiI6xoT","children":[],"key":"jqMKPyJSe2"}],"key":"YUvi7lDIu8"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"YZzMlW2on2"},{"type":"outputs","id":"3Joz7Uo2LZH0RlCEQJx3I","children":[],"key":"TpLtPYxJTd"}],"key":"ZxkWYqRCXW"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"FdQhN3ZX0J"},{"type":"outputs","id":"fRlYoZ_MHXzyaXIX-UlS9","children":[],"key":"kVuQifrjNV"}],"key":"q5OGnO4ZzM"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"baLdG2LoYb"},{"type":"outputs","id":"Pq_60RliWZYQq0ldEwWU7","children":[]