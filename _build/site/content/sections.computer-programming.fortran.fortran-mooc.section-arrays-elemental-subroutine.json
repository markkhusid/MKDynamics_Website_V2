 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"bI7ZOhx0ws"}],"key":"SOeiIdxCiE"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"U1YQcupzkS"}],"key":"zW37AgsF3G"}],"key":"Gb9TfpBYBi"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"ngKDk1cOvR"},{"type":"outputs","id":"ZDW_TpN5yi6DAlJn8Jzk6","children":[],"key":"eVFks68x96"}],"key":"UguUbCBszg"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"tBO8Vlk9Yl"},{"type":"outputs","id":"6Ze0OmzGO2_9TQz9_OyaI","children":[],"key":"J5SDxEtJJl"}],"key":"w8aweguXhC"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"K03gGGkcdr"},{"type":"outputs","id":"BA6Om-pa6eshohX606wrx","children":[],"key":"GSjg2jqdAg"}],"key":"U3XXWQq6l0"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"c5jq4zeX2z"},{"type":"outputs","id":"0y8Sw74_YYuAju4cHT9-N","children":[]