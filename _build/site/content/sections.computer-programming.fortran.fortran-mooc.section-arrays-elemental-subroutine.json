 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"Cgzf1cHVVL"}],"key":"H0H19VaArv"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"yEMbXLOfNF"}],"key":"eFHjDLbxh4"}],"key":"Nd9SkP8aJD"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"NUTw4GbPdC"},{"type":"outputs","id":"r7D-jD0Fgmho1DPTguwdf","children":[],"key":"K6kSrLvc39"}],"key":"xMulsaNY4l"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"a1UXXkSa8t"},{"type":"outputs","id":"15W8Zl9hXN45EZOpowfCH","children":[],"key":"mnBKHr1I4z"}],"key":"L48f2JZzwx"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"NmNLOPWKa5"},{"type":"outputs","id":"NE8ybdjfYZhTF3we1NUVu","children":[],"key":"r8zaHMluW3"}],"key":"MtyGzTxlno"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"YdpsK6rHja"},{"type":"outputs","id":"111ZWhXTsXr5I9Whub_-C","children":[]