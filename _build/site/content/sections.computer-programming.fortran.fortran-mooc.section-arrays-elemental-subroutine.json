 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"OYeA31LEbv"}],"key":"SZ0UazHvTH"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"KMPCjqWnm7"}],"key":"PVTLZkLQEP"}],"key":"hveyP0dX8Z"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"zAFqtbWRlr"},{"type":"outputs","id":"EUqI0L8cUAKqg6aKaSbe8","children":[],"key":"cgnhOBotcD"}],"key":"DYHcOdrWRg"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"gy1wq6MmFE"},{"type":"outputs","id":"ZesE--TOOGdkJEICPMpUU","children":[],"key":"W6wmPdMTgS"}],"key":"L73PKXdoYN"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"glAS7o0jWN"},{"type":"outputs","id":"rlunIvCD4x83kH75c-Daa","children":[],"key":"De2fuKGqZR"}],"key":"A0gtRWaeWx"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"SQjQRYfHHG"},{"type":"outputs","id":"Yy63Z8pZIo_CNJUrKU17Z","children":[]