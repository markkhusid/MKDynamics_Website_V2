 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"fIePA0WU4d"}],"key":"H1nGA6I0ZC"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"wOSvif9TfS"}],"key":"R6lEYmc7mV"}],"key":"nGypySKlcH"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"WK8cJraUvV"},{"type":"outputs","id":"FAkPjIFw_hRVQFJUe803c","children":[],"key":"VYBPQPDcCS"}],"key":"ZRpDuUrRTA"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"sAbqPFS5RA"},{"type":"outputs","id":"GOCSE3TamY18exlg1LZmk","children":[],"key":"uyk0l0GNi1"}],"key":"H0RCmz3hFL"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"zAeZhxpuRG"},{"type":"outputs","id":"dwRLMsJZhUr6isWYOurUJ","children":[],"key":"TqHH24hnmi"}],"key":"ldSMGzRBKR"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"qVahLdIL84"},{"type":"outputs","id":"M4nd1Xc1jSphObthvZJMU","children":[]