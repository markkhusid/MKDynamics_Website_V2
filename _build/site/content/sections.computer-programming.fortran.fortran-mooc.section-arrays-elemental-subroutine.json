 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"hTBwdNRuWH"}],"key":"s0g1dAE6zP"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"eznKhMcwPS"}],"key":"lOYjyloV1q"}],"key":"EJ0jJLSTui"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"jWWOVbSp3q"},{"type":"outputs","id":"Vr8UAUxORsSL7i53if2Tr","children":[],"key":"m5b7IttXh0"}],"key":"eNh04WNEgI"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"fctaAvzkSw"},{"type":"outputs","id":"ovR-VU9FY_y7YJYKJGy47","children":[],"key":"kGs95YJVBp"}],"key":"NqulX62508"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"Qo4NF7IF38"},{"type":"outputs","id":"UIOVQ_qRxLoXAkYx_4Hej","children":[],"key":"w9BLqAtCgv"}],"key":"RxGIQL4XkB"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"ePHwmOE6IS"},{"type":"outputs","id":"3ZigeZvopydLTrOtOHe2P","children":[]