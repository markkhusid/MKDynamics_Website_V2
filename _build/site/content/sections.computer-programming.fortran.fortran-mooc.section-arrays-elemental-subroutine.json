 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"IQIXhgPv8V"}],"key":"C0SScaqJLo"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"OgFTRJOrFL"}],"key":"SwlDO8hPSo"}],"key":"sRL0myzs4M"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"WSZGorBcz0"},{"type":"outputs","id":"tE2nXjsObOGt_HTRbrxP5","children":[],"key":"LWRPSFumOU"}],"key":"YdcM7ca7Id"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"rmgLPLytWd"},{"type":"outputs","id":"BFHJnrulwDDjJrT3X8DGx","children":[],"key":"sokYm4A1fe"}],"key":"vkTIn5OaBj"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"gOUO5t84jK"},{"type":"outputs","id":"bEvCu8MLlIFmAkePMUJv9","children":[],"key":"mrvc58OfCb"}],"key":"BVxmwhlG4t"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"yUsHlZgl8Z"},{"type":"outputs","id":"B1pC5H-nulBRUVQWBrYW1","children":[]