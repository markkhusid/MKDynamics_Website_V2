 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"Y9HpSMCkwv"}],"key":"vM16MDyXaF"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"liDrBlSYTJ"}],"key":"ogPPL9GCRH"}],"key":"j8exEr5awd"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"gMEgGQnqHJ"},{"type":"outputs","id":"A0fde5JN4yug7B6lHaJmv","children":[],"key":"Oop2KsXsDa"}],"key":"C6NMsp6o7z"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"XDcJ3aUFoR"},{"type":"outputs","id":"sIjIigqvRPIgeK9lVt2-T","children":[],"key":"bkooytcFE9"}],"key":"ZSXIOZqm3T"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"YVUPdXeiSb"},{"type":"outputs","id":"AeHKzS554Lcg5c8ki0_H2","children":[],"key":"h7ff6t81k2"}],"key":"zIweXTdvGb"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"VNnfIyxzRg"},{"type":"outputs","id":"Wm7qYgfRzqnYuMucBn1hU","children":[]