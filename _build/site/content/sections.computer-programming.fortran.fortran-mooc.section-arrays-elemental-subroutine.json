 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"eNWaHrM4vO"}],"key":"EkPWF0TwcR"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"OHGvHYVaBK"}],"key":"DOYAdK79Ii"}],"key":"acMGuQcbeG"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"PIAuYFE4iE"},{"type":"outputs","id":"HRE_df6XV3U1Tb9SI3H_m","children":[],"key":"N7m1yOiv2R"}],"key":"gTkUXa8AGo"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"uOJ3gb7buA"},{"type":"outputs","id":"7tyCBmx8_d6c5AyjAEnYL","children":[],"key":"SdFuJMEGnF"}],"key":"mxtJMygt1i"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"T8xG8c1y3A"},{"type":"outputs","id":"4w86uT7QqD-YL_FfxgKDD","children":[],"key":"H76UDSVlJb"}],"key":"Td6m3iJO2k"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"TP4XJxS42a"},{"type":"outputs","id":"BlZdwJEzsDeYUIVFQWud4","children":[]