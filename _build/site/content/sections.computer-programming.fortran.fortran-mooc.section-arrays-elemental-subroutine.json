 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"cukVrIWBx1"}],"key":"qtfOVDkwjP"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Uy3zOa0zTB"}],"key":"AMIBMOTKzo"}],"key":"sloQheRkmX"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"aDCu1J16B0"},{"type":"outputs","id":"XLKcX1U54qE9u-Zt6UgoV","children":[],"key":"fiQyETazSL"}],"key":"CvnHR2UcNr"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"LQX1QZmXxa"},{"type":"outputs","id":"ElxIdRx0fxBPDG4wgjIyL","children":[],"key":"rYp68HNBdG"}],"key":"pqeW8sKQ98"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"uiH5clRu4d"},{"type":"outputs","id":"RoXGwBHyQotwC8TBlF6t1","children":[],"key":"ocdbvBlfr0"}],"key":"tMSA0ujvEE"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"Zkyus2lBtC"},{"type":"outputs","id":"ou8szMeBq_QzhjinhwiQ9","children":[]