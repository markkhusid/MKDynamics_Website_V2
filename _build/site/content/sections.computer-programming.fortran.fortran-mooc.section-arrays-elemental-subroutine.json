 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"dk9UdooUhz"}],"key":"FS45KRg9ci"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Ecwm7Ltn7T"}],"key":"v2jIoormv5"}],"key":"jDqr50CApb"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"aKuvH54N0D"},{"type":"outputs","id":"PDjAkNbcfH1SSX7tGZnxm","children":[],"key":"PqTmF6zLXi"}],"key":"G1hVQhDxLf"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"fvAJp0EUpy"},{"type":"outputs","id":"8T_8ZcfQJgVK6Lh1dABPC","children":[],"key":"CwY2Nn5FIx"}],"key":"bfzcD4F6aV"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"Z1y5lXNPL4"},{"type":"outputs","id":"vl2HLj1ipkA7bQWsuabDN","children":[],"key":"EBdjM0Nfe5"}],"key":"FCFEswS0rQ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"HbwcTqunl3"},{"type":"outputs","id":"Hw_pPywLA0jiz9zDd_qW2","children":[]