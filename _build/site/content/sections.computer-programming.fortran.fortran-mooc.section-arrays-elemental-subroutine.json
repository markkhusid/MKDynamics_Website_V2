 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"axkLsN3QaH"}],"key":"PQi2oRj0b4"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"feRx93dJmJ"}],"key":"mUJ9KM5uV6"}],"key":"fI6Y5Ja5A0"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"RzxA4bpCRp"},{"type":"outputs","id":"Wm9sO9p3BbgRRFlph9PJP","children":[],"key":"V0U6f0aY3K"}],"key":"l9UCjN4Ge4"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"pkz3JpGgUR"},{"type":"outputs","id":"RrcwIOj6W_tkkgfseg1da","children":[],"key":"POfTA2BjnW"}],"key":"bgMaAeMTjJ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"rHsHqi9Ee8"},{"type":"outputs","id":"M_NOm-FoGpLqjNYJDxOb1","children":[],"key":"YTxHF61BPc"}],"key":"VjRRqtyjqj"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"zjijCcZBCe"},{"type":"outputs","id":"9hXDrkFyHlo961GpIEoTE","children":[]