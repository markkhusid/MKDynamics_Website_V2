 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"mZLwg68BF4"}],"key":"FDbAq8s08l"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Y5zjhEDJsI"}],"key":"zkY1V33Em4"}],"key":"fAhXVQYTOJ"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"VpvNRtyNaA"},{"type":"outputs","id":"TP_5OL6MGvGdoazTglaH9","children":[],"key":"OXxiqoGtsI"}],"key":"x8cS8lgUw9"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"KcYZnSJi5u"},{"type":"outputs","id":"sKcMVCZT6aXgiNOLoTUyZ","children":[],"key":"KPLzfOAVAU"}],"key":"J0NlTz8ziB"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"e5E9sajQ5t"},{"type":"outputs","id":"7Ki4ODRY_a6ckTlzDl9pX","children":[],"key":"TOLOSlntAL"}],"key":"pLQ5mC90ox"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"CrcsVFpPlv"},{"type":"outputs","id":"aVNc9WT6E66KA2poLdOHV","children":[]