 i = 1, 3\n        print '(*(F7.2))', B(i, :)\n    end do\n\n    print '(A)', 'C'\n    do i = 1, 3\n        print '(*(F7.2))', C(i, :)\n    end do\n\ncontains\n\n    elemental subroutine compute(A, B, C)\n        implicit none\n        real, intent(in) :: A\n        real, intent(out) :: B\n        real, intent(out) :: C\n\n        B = 2*A\n        C = 3*A\n    end subroutine compute\n\nend program elemental_subroutine","position":{"start":{"line":1,"column":1},"end":{"line":37,"column":1}},"key":"X6SAnjX9xn"}],"key":"JQN1mWkAFV"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"The above program is compiled and run using Fortran Package Manager (fpm):","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"chd5l2NtVu"}],"key":"vmyQ2nGu1a"}],"key":"EgHS06rxlU"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"import os\nroot_dir = os.getcwd()","key":"H9V7RLAo7z"},{"type":"outputs","id":"G2fI588Wh0g4UGbqymlq_","children":[],"key":"RBBdUq8L2m"}],"key":"eun7dHLHln"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"code_dir = root_dir + \"/\" + \"Fortran_Code/Section_Arrays_Elemental_Subroutine\"","key":"HrUTBBiUke"},{"type":"outputs","id":"7rUYoebnVF4sc7j7sOlMM","children":[],"key":"znq7R5TWxl"}],"key":"Z8j6T0rsSz"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"os.chdir(code_dir)","key":"CtdDtEBfJ3"},{"type":"outputs","id":"XONShG-GXHa359dpt5nDc","children":[],"key":"as4cjV4if4"}],"key":"kyJVCB4hTT"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"build_status = os.system(\"fpm build 2>/dev/null\")","key":"JFLSNnfEsE"},{"type":"outputs","id":"B1M8jpl7p39yHo49BM-16","children":[]