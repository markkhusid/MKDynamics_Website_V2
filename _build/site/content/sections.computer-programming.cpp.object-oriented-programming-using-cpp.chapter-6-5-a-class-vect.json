\n\nint main()\n{\n   vect a(60), b[20];\n\n   b[1].element(5) = 7;\n   cout << b[1].element(5) << \" 1 element 5\\n\";\n   for (int i = 0; i <= a.ub(); ++i) {\n      a.element(i) = i;\n      cout << a.element(i) << \"\\t\";\n   }\n   cout << endl;     // Added for output clarity\n}","position":{"start":{"line":1,"column":1},"end":{"line":30,"column":1}},"key":"kbdLsg2Teu"}],"key":"Boi3fPCAFj"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"In file vect1.h","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"t0M309khvT"}],"key":"aofsqHfytS"}],"key":"dL0QlGLJY6"},{"type":"block","kind":"notebook-content","children":[{"type":"code","lang":"C++","value":"/*********************************************************************\n\n  Filename:  vect1.h\n  Chapter:   6      Object Creation and Destruction\n  Section:   6.5    A class vect\n  Compiler:  Borland C++     Version 5.0       Summer 1996\n  Object Oriented Programming Using C++, Edition 2   By Ira Pohl\n\n*********************************************************************/\n\n//Implementation of a safe array type vect\n\n#include  <iostream>    // changed iostream.h to iostream. MK\n#include  <assert.h>\n\n//Implementation of a safe array type vect\nclass vect {\npublic:\n   explicit vect(int n = 10);\n   ~vect() { delete []p; }\n   int&  element(int i);                 //access p[i]\n   int  ub() const {return (size - 1);}  //upper bound\nprivate:\n   int*  p;\n   int   size;\n};\n\nvect::vect(int n) : size(n)\n{\n   assert(n > 0);\n   p = new int[size];\n   assert(p != 0);\n}\n\nint& vect::element(int i)\n{\n   assert (i >= 0 && i < size);\n   return p[i];\n}","position":{"start":{"line":1,"column":1},"end":{"line":41,"column":1}},"key":"r4IeA5mDYK"}],"key":"dM8KpLpJTq"},{"type":"block","kind":"notebook-content","children":[{"type":"heading","depth":2,"p